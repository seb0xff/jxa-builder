{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: cli",
      "type": "debugpy",
      "request": "launch",
      "module": "jxa_builder.cli",
      "justMyCode": true,
      "args": []
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": []
    }
  ]
}
